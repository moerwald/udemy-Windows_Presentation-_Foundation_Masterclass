<Window x:Class="DesktopContactApp.NewContactWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopContactApp"
        mc:Ignorable="d"
        WindowStyle="None"
        ResizeMode="NoResize"
        Title="NewContactWindows" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox x:Name="nameTb"></TextBox>
            <TextBox x:Name="emailTb"></TextBox>
            <TextBox x:Name="phoneTb"></TextBox>
            <Button Click="SaveButton_Click">Save</Button>
        </StackPanel>
        
    </Grid>
</Window>
